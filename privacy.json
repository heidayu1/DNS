      "type": "adblock",
      "transformations": [
        "RemoveComments",
        "Compress",
        "Validate"
      ]
    },
    {
      "name": "AdGuard CNAME disguised trackers list",
      "source": "https://raw.githubusercontent.com/AdguardTeam/cname-trackers/master/data/combined_disguised_trackers.txt",
      "type": "adblock",
      "transformations": [
        "RemoveComments",
        "Compress",
        "Validate"
      ]
    },
    {
      "name": "EasyPrivacy",
      "source": "https://raw.githubusercontent.com/easylist/easylist/gh-pages/easyprivacy.txt",
      "type": "adblock",
      "transformations": [
        "RemoveComments",
        "Compress",
        "Validate"
      ]
    }
  ],
  "transformations": [
    "RemoveComments",
    "Validate",
    "Deduplicate",
    "TrimLines",
    "InsertFinalNewLine",
    "RemoveEmptyLines"
  ]
