{
  "name": "AdGuard Fusion",
  "description": "DNS block list that can be used for AdGuardHome. Source from https://github.com/badmojr/1Hosts, https://github.com/hagezi/dns-blocklists .",
  "homepage": "https://github.com/heidayu1/DNS",
  "license": "GPLv3",
  "version": "1.0.0.0",
  "sources": [
    {
      "name": "1Host Lite",
      "source": "https://badmojr.github.io/1Hosts/Lite/adblock.txt",
      "type": "adblock",
      "transformations": [
        "RemoveComments",
        "Compress",
        "Validate"
      ]
    },
    {
      "name": "HaGeZi's Pro Mini",
      "source": "https://cdn.jsdelivr.net/gh/hagezi/dns-blocklists@latest/adblock/pro.mini.txt",
      "type": "adblock",
      "transformations": [
        "RemoveComments",
        "Compress",
        "Validate"
      ]
    }
  ],
  "transformations": [
    "RemoveComments",
    "Compress",
    "Validate",
    "Deduplicate",
    "TrimLines",
    "InsertFinalNewLine",
    "RemoveEmptyLines"
  ]
}